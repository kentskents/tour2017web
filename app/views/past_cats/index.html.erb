<p id="notice"><%= notice %></p>

<h1>Past Cats</h1>

<table>
  <thead>
    <tr>
      <th>Cat</th>
      <th>Whereabouts</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @past_cats.each do |past_cat| %>
      <tr>
        <td><%= past_cat.cat %></td>
        <td><%= past_cat.whereabouts %></td>
        <td><%= link_to 'Show', past_cat %></td>
        <td><%= link_to 'Edit', edit_past_cat_path(past_cat) %></td>
        <td><%= link_to 'Destroy', past_cat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Past Cat', new_past_cat_path %>
